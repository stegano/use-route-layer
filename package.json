  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "babel-preset-minify": "^0.5.1"
  },
  "dependencies": {
    "@types/react": "^16.9.49",
    "query-string": "^6.13.1",
    "react": "^16.13.1"
  },
  "keywords": [
    "layer",
    "react-router",
    "react-router-layer",
    "next-router",
    "next-router-layer"
  ],
  "repository": {
    "type": "git",
